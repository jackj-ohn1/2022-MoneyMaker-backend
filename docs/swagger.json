                "summary": "\"获取某个商品的所有评论\"",
                "parameters": [
                    {
                        "type": "string",
                        "description": "goodsid",
                        "name": "goodsid",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{\"all\":所有分值情况, \"infor\":\"评论信息以及学号\"}",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/tables.Comment"
