                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Search"
                ],
                "summary": "\"搜索\"",
                "parameters": [
                    {
                        "description": "content",
                        "name": "content",
